---
import '../style/global.css';
import Cursor from '../components/Cursor.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
// import { ClientRouter } from "astro:transitions";

export interface Props {
  subsection?: boolean;
  title?: string;
  active?: string;
  subsectionActive?: number;}

const { subsection = false, title = 'interdependence â€“ exploring opposites', active="0", subsectionActive = 0  } = Astro.props as Props;

---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.png" />
    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
    <!-- <ClientRouter /> -->

  </head>
<body>

  <div class="content-wrapper">
    <slot />
    <Footer></Footer>
    <Header subsection={subsection} active={active} subsectionActive={subsectionActive}></Header>
  </div>
  <Cursor></Cursor>



</body>
</html>


